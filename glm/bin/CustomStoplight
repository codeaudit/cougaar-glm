#!/bin/csh -f

source $ALP_INSTALL_PATH/bin/setlibpath.csh

set MYCLASSES="ui.planviewer.stoplight.SupplyController"
set MYPROPERTIES="-Dalp.install.path=$ALP_INSTALL_PATH"

set osargs=""
set os=`uname`
if ("$os" == "Linux") then
  set osargs="-green"
endif

echo java $osargs $MYPROPERTIES -classpath $LIBPATHS $MYCLASSES

java  $osargs $MYPROPERTIES -classpath $LIBPATHS $MYCLASSES $*


